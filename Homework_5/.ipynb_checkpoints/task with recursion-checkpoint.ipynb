    "    if k == (len(l)):\n",
    "        return\n",
    "    print(l[k])\n",
    "    print_list(l, k + 1)\n",
    "\n",
    "\n",
    "list_1 = input().split()\n",
    "\n",
    "print(recursive_print_list(list_1))"
